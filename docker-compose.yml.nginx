version: '3'
services:
  app:
    networks:
      nginx-deploy:
        aliases:
          - ${PROJECT_NAME}_${ENV}
    volumes:
      - nginx-deploy:/deploy
      - ./deploy/${PROJECT_NAME}_${ENV}_nginx/log:/deploy/${PROJECT_NAME}_${ENV}${NEW}/log
volumes:
  nginx-deploy:
    external: true
networks:
  nginx-deploy:
    external: true

